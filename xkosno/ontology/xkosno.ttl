@prefix : <https://data.norge.no/vocabulary/xkosno#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prof: <http://www.w3.org/ns/dx/prof/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix xkos: <http://rdf-vocabulary.ddialliance.org/xkos#> .
@prefix schema: <https://schema.org/> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix uneskos: <http://purl.org/umu/uneskos#> .
@prefix iso-thes: <http://purl.org/iso25964/skos-thes#> .
@base <https://data.norge.no/vocabulary/xkosno#> .

<https://jimjyang.github.io/playground/xkosno/ontology/xkosno> rdf:type owl:Ontology ;
                                                                owl:versionIRI <https://jimjyang.github.io/playground/xkosno/ontology/xkosno/0.1.000> ;
                                                                dct:license <http://creativecommons.org/licenses/by/4.0/> ;
                                                                vann:preferredNamespacePrefix "xkosno" ;
                                                                vann:preferredNamespaceUri : ;
                                                                voaf:extends <http://rdf-vocabulary.ddialliance.org/xkos> ;
                                                                rdfs:comment "Dette er ontologien for XKOS-AP-NO."@nb ,
                                                                             "This is the ontology for XKOS-AP-NO."@en ;
                                                                rdfs:label "Spesifikasjon for klassifikasjonsbeskrivelser (XKOS-AP-NO)"@nb ,
                                                                           "XKOS-AP-NO: Norwegian Application Profile of XKOS"@en ;
                                                                prof:isProfileOf <http://rdf-vocabulary.ddialliance.org/xkos> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty ;
            rdfs:range rdfs:Literal ,
                       foaf:Agent .


###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:AnnotationProperty ;
               rdfs:range rdfs:Literal .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
dct:publisher rdf:type owl:AnnotationProperty ;
              rdfs:range rdfs:Literal ,
                         xsd:anyURI .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty ;
          rdfs:range rdf:langString ,
                     rdfs:Literal .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/usageNote
vann:usageNote rdf:type owl:AnnotationProperty .


###  http://purl.org/vocommons/voaf#extends
voaf:extends rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty ;
               rdfs:range rdf:langString ,
                          rdfs:Literal .


###  http://www.w3.org/ns/dx/prof/isProfileOf
prof:isProfileOf rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/mbox
foaf:mbox rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/subject
dct:subject rdf:type owl:ObjectProperty ;
            rdfs:domain skos:Concept ;
            rdfs:range skos:Concept ;
            rdfs:comment "Egenskapen brukes til å referere til sentralt begrep som er viktig for å forstå og tolke kategorien."@nb ,
                         "This property is used to refer to the concept which is important in order to understand the category."@en ;
            rdfs:label "refererer til begrep"@nb ,
                       "refers to concept"@en ;
            skos:example "Kategorien med koden «A» i «Næringsgruppering 2007» refererer til begrepet ‘primærnæring’."@nb .


###  http://purl.org/umu/uneskos#contains
uneskos:contains rdf:type owl:ObjectProperty ;
                 owl:inverseOf skos:inScheme ;
                 rdfs:domain skos:ConceptScheme ;
                 rdfs:range skos:Concept ;
                 vann:usageNote "Når en flat klassifikasjon ikke tar med det ene klassifikasjonsnivået, er denne egenskapen den eneste måte å si hvilke kategorier en klassifikasjon inneholder."@nb ,
                                "When the one classifcation level in a flat classification is not specified, this property is the only way to speficiy which categories that are in the classification."@en ;
                 rdfs:comment "Egenskapen brukes til å referere til kategori(er) som en klassifikasjon inneholder."@nb ,
                              "This property is used to refer to the classification items that the classification contains."@en ;
                 rdfs:isDefinedBy uneskos:contains ;
                 rdfs:label "contains classification items"@en ,
                            "inneholder kategori"@nb ;
                 skos:example "«Standard for sivilstand 1993» inneholder kategoriene «1 – Ugift», …​, «9 – Gjenlevende partner»"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#belongsTo
xkos:belongsTo rdf:type owl:ObjectProperty ;
               rdfs:domain skos:ConceptScheme ;
               rdfs:range skos:Concept ;
               vann:usageNote "I XKOS er rdfs:Resource (en hvilken som helst type ressurs) brukt som range, og det er samtidig anbefalt å referere et begrep (en instans av skos:Concept). Det er derfor i dette dokumentet brukt skos:Concept som range."@nb ,
                              "XKOS does not declare a formal range for xkos:belongsTo, and does not define a class to represent the classification itself; it is recommended to model it as an instance of skos:Concept that will serve as an entry in statistical classification registries, but another class could be used as well."@en ;
               rdfs:comment "Egenskapen brukes til å knytte en hovedversjon av klassifikasjonen til f.eks. en «klassifikasjonsfamilie» eller en «klassifikasjonsserie»."@nb ,
                            "This property is used to connect a major version of a classification to a concept representing the overall classification."@en ;
               rdfs:isDefinedBy xkos:belongsTo ;
               rdfs:label "belongs to"@en ,
                          "tilhører"@nb ;
               skos:example "«Næringsgruppering 2007 (SN2007)» tilhører klassifikasjonsfamilien «Standard for næringsgruppering (SN)»"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#compares
xkos:compares rdf:type owl:ObjectProperty ;
              rdfs:domain xkos:Correspondence ;
              rdfs:range skos:ConceptScheme ;
              rdfs:comment "Egenskapen brukes til å referere til klassifikasjonene (eller versjonene/variantene av samme klassifikasjon) som det sammenlignes mellom."@nb ,
                           "This property is used to link directly the xkos:Correspondence to the classification schemes that it puts in relation."@en ;
              rdfs:isDefinedBy xkos:compares ;
              rdfs:label "compares"@en ,
                         "sammenligner"@nb ;
              skos:example "«Klassifisering av informasjonssektoren 2007» sammenlignes med «Næringsgruppering 2007 (SN2007)»."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#covers
xkos:covers rdf:type owl:ObjectProperty ;
            rdfs:domain xkos:ClassificationLevel ,
                        skos:ConceptScheme ;
            rdfs:range skos:Concept ;
            vann:usageNote "A well-defined classification should cover max. one domain, subject field or such like."@en ,
                           "Bruk heller egenskapen Klassifikasjon – dekker uttømmende (xkos:coversExhaustively) når klassifikasjonen dekker domenet/fagområdet uttømmende, og/eller Klassifikasjon – dekker gjensidig utelukkende (xkos:coversMutuallyExclusively) når klassifikasjonen dekker domenet/fagområdet gjensidig utelukkende."@nb ,
                           "En veldefinert klassifikasjon bør dekke maks. ett domene/fagområde/el.l."@nb ,
                           "Use rather the property xkos:coversExhaustively when the classification covers the domain / subject field exhaustively, and/or xkos:coversMutuallyExclusively when the classification covers the domain / subject field mutually exclusively."@en ;
            rdfs:comment "A classification / classification leval covers a defined field: economic activity, occupations, living organisms, etc. XKOS specifies the xkos:covers property to express this relation."@en ,
                         "Egenskapen brukes til å referere til begrep som beskriver det domene/fagområde/el.l. som klassifikasjonen/klassifikasjonsnivået dekker."@nb ;
            rdfs:isDefinedBy xkos:covers ;
            rdfs:label "covers"@en ,
                       "dekker"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#coversExhaustively
xkos:coversExhaustively rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf xkos:covers ;
                        vann:usageNote "A well-defined classification should cover max. one domain, subject field or such like, and exhaustively. This property should derfor be used for a well-defined classification."@en ,
                                       "En veldefinert klassifikasjon bør dekke maks. ett domene/fagområde/el.l., og uttømmende. Denne egenskapen bør derfor alltid brukes for en veldefinert klassifikasjon."@nb ;
                        rdfs:comment "Egenskapen brukes til å uttrykke at klassifikasjonen dekker et domene/fagområde/el.l. uttømmende, samt å referere til det som dekkes."@nb ,
                                     "If the coverage of the given field is complete (i.e. all notions in the field can potentially be classified under the classification), we say that the coverage is exhaustive."@en ;
                        rdfs:isDefinedBy xkos:coversExhaustively ;
                        rdfs:label "covers exhaustively"@en ,
                                   "dekker uttømmende"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#coversMutuallyExclusively
xkos:coversMutuallyExclusively rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf xkos:covers ;
                               vann:usageNote " A well-defined classification should cover max. one domain, subject field or such like, and with mutually exclusive categories."@en ,
                                              "A well-defined classification usually covers its domain, subject field or such like both exhaustively and mutually exclusively. In such cases this property and the property xkos:coversExhaustively should be used."@en ,
                                              "En veldefinert klassifikasjon bør dekke maks. ett domene/fagområde/el.l., og med gjensidig utelukkende kategorier."@nb ,
                                              "En veldefinert klassifikasjon dekker sitt domene/område/begrep både uttømmende og gjensidig utelukkende. I slike tilfeller bør både denne egenskapen og egenskapen Klassifikasjon – dekker uttømmende (xkos:coversExhaustively) brukes."@nb ;
                               rdfs:comment "If there is no overlap between the classification items at a given level of the classification, we say that the concepts (skos:Concept) representing the items are mutually exclusive."@en ,
                                            "På ethvert nivå i en veldefinert klassifikasjon er kategoriene gjensidig utelukkende. Denne egenskapen brukes til å uttrykke dette, samt å referere til begrep som kategoriene dekker."@nb ;
                               rdfs:isDefinedBy xkos:coversMutuallyExclusively ;
                               rdfs:label "covers mutually exclusively"@en ,
                                          "dekker gjensidig utelukkende"@nb ;
                               skos:example "Næringsgruppering 2007 dekker begrepet ‘økonomisk aktivitet’ med gjensidig utelukkende kategorier (og uttømmende)."@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#generalizes
xkos:generalizes rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf skos:narrower ;
                 owl:inverseOf xkos:specializes ;
                 rdfs:comment "Egenskapen brukes til å referere til kategorier som den aktuelle kategorien generaliserer, i en generisk relasjon mellom kategoriene."@nb ,
                              "This property is used to refer to categories which the category in question generalizes, in a generic relationship between the categories."@en ;
                 rdfs:isDefinedBy xkos:generalizes ;
                 rdfs:label "generaliserer"@nb ,
                            "generalizes"@en ;
                 skos:example "Kategorien med kode «A» på nivå 1 i klassifikasjonen «Næringsgruppering 2007» generaliserer kategoriene med kode «01», «02» og «03» på nivå 2."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#hasPart
xkos:hasPart rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf skos:narrower ;
             owl:inverseOf xkos:isPartOf ;
             rdfs:comment "Egenskapen brukes til å referere til kategorier som den aktuelle kategorien består av, i en partitiv relasjon mellom kategoriene."@nb ,
                          "This property is used to refer to categories which the category in question consists of, in a partitive relationship between the categories."@en ;
             rdfs:isDefinedBy xkos:hasPart ;
             rdfs:label "består av"@nb ,
                        "har del"@nb ,
                        "has part"@en ;
             skos:example "'ukedag' består av 'mandag', 'tirsdag', 'onsdag', 'torsdag', 'fredag', 'lørdag' og 'søndag'."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#isPartOf
xkos:isPartOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf skos:broader ;
              rdfs:comment "Egenskapen brukes til å referere til kategorier som den aktuelle kategorien er del av, i en partitiv relasjon mellom kategoriene."@nb ,
                           "This property is used to refer to categories which the category in question is part of, in a partitive relationship between the categories."@en ;
              rdfs:isDefinedBy xkos:isPartOf ;
              rdfs:label "er del av"@nb ,
                         "is part of"@en ;
              skos:example "'mandag' er del av 'ukedag'."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#levels
xkos:levels rdf:type owl:ObjectProperty ;
            rdfs:domain skos:ConceptScheme ;
            rdfs:range rdf:List ;
            rdfs:comment "Egenskapen brukes til å referere til en liste av nivåene i klassifikasjonen (instanser av Klassifikasjonsnivå xkos:ClassificationLevel), representert som en sortert RDF-liste (instans av rdf:List)."@nb ,
                         "This property is used to refer to the list of the classification levels represented as an RDF list of ordered levels (instances of ClassificationLevel)."@en ;
            rdfs:isDefinedBy xkos:levels ;
            rdfs:label "har klassifikasjonsnivå"@nb ,
                       "level list"@en ;
            skos:example "Innholdet i «Næringsgruppering 2007» som en (nøstet) liste, med klassifikasjonsnivåene 1 til 5."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#madeOf
xkos:madeOf rdf:type owl:ObjectProperty ;
            rdfs:domain xkos:Correspondence ;
            rdfs:range xkos:ConceptAssociation ;
            rdfs:comment "Egenskapen brukes til å referere til kategorisammenligning, dvs. sammenligning på kategorinivå."@nb ,
                         "This property is used to group category correspondences that this correspondence is made of."@en ;
            rdfs:isDefinedBy xkos:madeOf ;
            rdfs:label "har kategorisammenligning"@nb ,
                       "made of"@en ;
            skos:example "Klassifikasjonssammenligningen «Klassifisering av informasjonssektoren 2007 - Næringsgruppering 2007 (SN 2007)» inneholder bl.a. sammenligning mellom kategoriene «26.1 - Produksjon av elektroniske komponenter og kretskort» i «Standard for næringsgruppering (SN2007)» og «1.11 - Produksjon av elektroniske komponenter og kretskort» i «Klassifisering av informasjonssektoren 2007»."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#next
xkos:next rdf:type owl:ObjectProperty ;
          rdfs:domain skos:Concept ;
          rdfs:range skos:Concept ;
          vann:usageNote "Ordene «neste» og «bak» bør forståes innenfor konteksten av det aktuelle klassifikasjonsnivået i den aktuelle klassifikasjonen."@nb ,
                         "The words \"next\" and \"after\" should be understood within the context of that particular classification level in that particular classification."@en ;
          rdfs:comment "Egenskapen brukes til å referere til kategorien som er bak den aktuelle kategorien, i en sekvensiell relasjon mellom kategoriene."@nb ,
                       "This property is used to refer to the category that is after the category in question, in a sequential relation between the categories."@en ;
          rdfs:isDefinedBy xkos:next ;
          rdfs:label "neste kategori"@nb ,
                     "next"@en ;
          skos:example "Den neste kategori for «A» er «B» i Nivå 1 av «Næringsgruppering 2007 (SN 2007)»."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#organizedBy
xkos:organizedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain xkos:ClassificationLevel ;
                 rdfs:range skos:Concept ;
                 rdfs:comment "Egenskapen brukes til å referere til begrep som kategoriene i klassifikasjonsnivået er klassifisert innen."@nb ,
                              "This property is used to specify the name (or nature, or type) of the items that constitute the level"@en ;
                 rdfs:isDefinedBy xkos:organizedBy ;
                 rdfs:label "er klassifisert innen"@nb ,
                            "organized by"@en ;
                 skos:example "«Næringsgruppering 2007» er klassifisert innen: nivå 1 = Næringshovedområde, nivå 2 = Næring, nivå 3 = Næringshovedgruppe, nivå 4 = Næringsgruppe og nivå 5 = Næringsundergruppe."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#previous
xkos:previous rdf:type owl:ObjectProperty ;
              rdfs:domain skos:Concept ;
              rdfs:range skos:Concept ;
              vann:usageNote "Ordene «forrige» og «foran» bør forståes innenfor konteksten av det aktuelle klassifikasjonsnivået i den aktuelle klassifikasjonen."@nb ,
                             "The words \"previous\" and \"before\" should be understood within the context of that particular classification level in that particular classification."@en ;
              rdfs:comment "Egenskapen brukes til å referere til kategorien som er foran den aktuelle kategorien, i en sekvensiell relasjon mellom kategoriene."@nb ,
                           "This property is used to refer to the category that is before the category in question, in a sequential relation between the categories."@en ;
              rdfs:isDefinedBy xkos:previous ;
              rdfs:label "forrige kategori"@nb ,
                         "previous"@en ;
              skos:example "Den forrige kategori til «B» er «A» i Nivå 1 av «Næringsgruppering 2007 (SN 2007)»."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#sourceConcept
xkos:sourceConcept rdf:type owl:ObjectProperty ;
                   rdfs:domain xkos:ConceptAssociation ;
                   rdfs:range skos:Concept ;
                   rdfs:comment "Egenskapen brukes til å referere til en kildekategori i en kategorisammenligning."@nb ,
                                "This property is used to link to a source concept in the correspondence."@en ;
                   rdfs:isDefinedBy xkos:sourceConcept ;
                   rdfs:label "har kildekategori"@nb ,
                              "source concept"@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#specializes
xkos:specializes rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf skos:broader ;
                 rdfs:comment "Egenskapen brukes til å referere til kategorier som den aktuelle kategorien spesialiserer, i en generisk relasjon mellom kategoriene."@nb ,
                              "This property is used to refer to categories which the category in question specializes, in a generic relationship between the categories."@en ;
                 rdfs:isDefinedBy xkos:specializes ;
                 rdfs:label "specializes"@en ,
                            "spesialiserer"@nb ;
                 skos:example "Kategorien med kode «01» på nivå 2 i klassifikasjonen «Næringsgruppering 2007» spesialiserer kategorien med kode «A» på nivå 1."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#supersedes
xkos:supersedes rdf:type owl:ObjectProperty ;
                rdfs:domain skos:ConceptScheme ;
                rdfs:range skos:ConceptScheme ;
                rdfs:comment "Egenskapen brukes til å referere til en versjon av klassifikasjonen som denne versjonen erstatter."@nb ,
                             "This property is used to refer to a classification/version that this classification/version supersedes."@en ;
                rdfs:isDefinedBy xkos:supersedes ;
                rdfs:label "remplerstatterace"@nb ,
                           "supersedes"@en ;
                skos:example "«Næringsgruppering 2007 (SN2007)» erstatter «Næringsgruppering 2002 (SN2002)»."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#targetConcept
xkos:targetConcept rdf:type owl:ObjectProperty ;
                   rdfs:domain xkos:ConceptAssociation ;
                   rdfs:range skos:Concept ;
                   rdfs:comment "Egenskapen brukes til å referere til en målkategori i en kategorisammenligning."@nb ,
                                "This property is used to link to a target concept in the correspondence."@en ;
                   rdfs:isDefinedBy xkos:targetConcept ;
                   rdfs:label "har målkategori"@nb ,
                              "target concept"@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#variant
xkos:variant rdf:type owl:ObjectProperty ;
             rdfs:domain skos:ConceptScheme ;
             rdfs:range skos:ConceptScheme ;
             rdfs:comment "Egenskapen brukes til å referere til en variant av klassifikasjonen, dvs. klassifikasjonen tilpasset for et spesifikt bruksbehov, ved f.eks. å begrense dekningen, slå sammen eller splitte noen kategorier på et gitt klassifikasjonsnivå."@nb ,
                          "In certain circumstances, statisticians need to \"customize\" a classification scheme for a specific use, by restricting the coverage, merging or splitting certain items at a given level, etc. The xkos:variant property can be used to represent the relation between the base classification scheme and its variant(s)."@en ;
             rdfs:isDefinedBy xkos:variant ;
             rdfs:label "har variant"@nb ,
                        "variant"@en ;
             skos:example "«Næringsgruppering 2007» har bl.a. variant «Fastlands-Norge 2009»."@nb .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#first
rdf:first rdf:type owl:ObjectProperty ;
          rdfs:domain rdf:List ;
          rdfs:range xkos:ClassificationLevel ;
          rdfs:comment "This property is used to refer to the first element in an rdf:List."@en ;
          rdfs:isDefinedBy "http://www.w3.org/1999/02/22-rdf-syntax-ns#first" .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#rest
rdf:rest rdf:type owl:ObjectProperty ;
         rdfs:domain rdf:List ;
         rdfs:range rdf:List ;
         rdfs:comment "This property is used to refer to the \"tail\" in an rdf:List."@en ;
         rdfs:isDefinedBy "http://www.w3.org/1999/02/22-rdf-syntax-ns#rest" .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:ObjectProperty ;
             owl:inverseOf skos:narrower ;
             rdfs:domain skos:Concept ;
             rdfs:range skos:Concept ;
             vann:usageNote "This property is not explicitly specified in XKOS-AP-NO, but added here in order to have it as the super-property for the hiearchically relations (isPartOf/hasPart, specializes/generalizes)."@en ;
             rdfs:isDefinedBy skos:broader ;
             rdfs:label "broader"@en .


###  http://www.w3.org/2004/02/skos/core#hasTopConcept
skos:hasTopConcept rdf:type owl:ObjectProperty ;
                   owl:inverseOf skos:topConceptOf ;
                   rdfs:domain skos:ConceptScheme ;
                   rdfs:range skos:Concept ;
                   vann:usageNote "This property is not explicity specified in XKOS-AP-NO."@en ;
                   rdfs:isDefinedBy skos:hasTopConcept ,
                                    <skos:hasTopConcept> ;
                   rdfs:label "has top concept"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty ;
              rdfs:domain skos:Concept ;
              rdfs:range skos:ConceptScheme ;
              rdfs:comment "Egenskapen brukes til å knytte kategorien til en eller flere klassifikasjoner."@nb ,
                           "This property is used to connect the category to a classification of classification level."@en ;
              rdfs:isDefinedBy skos:inScheme ;
              rdfs:label "er i klassifikasjon"@nb ,
                         "in classification/scheme"@en ;
              skos:example "Kategorien med kode «A» er i klassifikasjonen «Næringsgruppering 2007»."@nb .


###  http://www.w3.org/2004/02/skos/core#member
skos:member rdf:type owl:ObjectProperty ;
            rdfs:domain xkos:ClassificationLevel ;
            rdfs:range skos:Concept ;
            rdfs:comment "Egenskapen brukes til å referere til kategorier som klassifikasjonsnivået inneholder."@nb ,
                         "This property is used to refer to the classification items that are in the classification level."@en ;
            rdfs:isDefinedBy skos:member ;
            rdfs:label "inneholder kategori"@nb ,
                       "member"@en ;
            skos:example "Nivå 1 i Næringsgruppering inneholder «A – Jordbruk, skogbruk og fiske», …, og «U – Internasjonale organisasjoner og organer»."@nb .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:ObjectProperty ;
              rdfs:domain skos:Concept ;
              rdfs:range skos:Concept ;
              vann:usageNote "This property is not explicitly specified in XKOS-AP-NO, but added here in order to have it as the super-property for the hiearchically relations (isPartOf/hasPart, specializes/generalizes)."@en ;
              rdfs:isDefinedBy skos:narrower .


###  http://www.w3.org/2004/02/skos/core#topConceptOf
skos:topConceptOf rdf:type owl:ObjectProperty ;
                  rdfs:domain skos:Concept ;
                  rdfs:range skos:ConceptScheme ;
                  vann:usageNote "Ordet «første» bør forståes innenfor konteksten av den aktuelle klassifikasjonen. I en veldefinert klassifikasjon skal derfor kun én kategori ha denne egenskapen."@nb ,
                                 "The word \"first\" should be understood within the context of that particular classification. In a well-defined classification, there skall therefore be only one category which has this property."@en ;
                  rdfs:comment "Egenskapen brukes til å referere til en klassifikasjon hvor denne kategorien er den første."@nb ,
                               "This property is used to refer to the classification in which the category is the first one."@nb ;
                  rdfs:isDefinedBy skos:topConceptOf ;
                  rdfs:label "er første kategori i"@nb ,
                             "first category in"@en ;
                  skos:example "«A – Jordbruk, skogbruk og fiske» er den første kategorien i «Næringsgruppering 2007 (SN2007)»"@nb .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:DatatypeProperty ;
                rdfs:domain xkos:Correspondence ,
                            skos:ConceptScheme ;
                rdfs:range rdf:langString ;
                rdfs:comment "Egenskapen brukes til å oppgi en kortfattet beskrivelse av klassifikasjon/sammenligningen. Egenskapen bør gjentas når beskrivelsen er i flere språk."@nb ,
                             "This property is used to give a short description of the classification/correspondence, repeated when the description is in different languages."@en ;
                rdfs:isDefinedBy dct:description ;
                rdfs:label "beskrivelse"@nb ,
                           "description"@en .


###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:DatatypeProperty ;
               rdfs:domain xkos:Correspondence ,
                           skos:Concept ,
                           skos:ConceptScheme ;
               rdfs:range rdfs:Literal .


###  http://purl.org/dc/terms/isReplacedBy
dct:isReplacedBy rdf:type owl:DatatypeProperty ;
                 rdfs:domain skos:Concept ;
                 rdfs:range xsd:anyURI ;
                 vann:usageNote "This property is not explicitly specified in XKOS-AP-NO, but included here in order to express the relation replaces/isReplacedBy with a concept outside of the current classification, referenced as an URI."@en ;
                 rdfs:isDefinedBy "http://purl.org/dc/terms/isReplacedBy" .


###  http://purl.org/dc/terms/issued
dct:issued rdf:type owl:DatatypeProperty ;
           rdfs:domain xkos:Correspondence ,
                       skos:ConceptScheme ;
           rdfs:range xsd:date ,
                      xsd:dateTime ;
           rdfs:comment "Egenskapen brukes til å oppgi dato/tid når klassifikasjonen/klassifikasjonssammenligningen ble tilgjengeliggjort."@nb ,
                        "This property is used to specify the date/time when the classification/correspondence was made accessible."@en ;
           rdfs:isDefinedBy dct:issued ;
           rdfs:label "issued"@en ,
                      "tilgjengeliggjøringsdato"@nb .


###  http://purl.org/dc/terms/language
dct:language rdf:type owl:DatatypeProperty ;
             rdfs:domain xkos:Correspondence ,
                         skos:ConceptScheme ;
             rdfs:range xsd:anyURI ;
             vann:usageNote "The value shall be chosen from EU’s controlled vocabulary for Language, https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/language"@en ,
                            "Verdien skal hentes fra EUs kontrollerte liste over Language, https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/language."@nb ;
             rdfs:comment "Egenskapen brukes til å oppgi språk som klassifikasjonen/klassifikasjonssammenligningen er utgitt i."@nb ,
                          "This property is used to specify the language(s) that the classification/correspondence is in."@en ;
             rdfs:isDefinedBy dct:language ;
             rdfs:label "language"@en ,
                        "språk"@nb .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:DatatypeProperty ;
             rdfs:domain xkos:Correspondence ,
                         skos:ConceptScheme ;
             rdfs:range xsd:date ,
                        xsd:dateTime ;
             rdfs:comment "Egenskapen brukes til å oppgi dato/tidspunkt når klassifikasjonen/klassifikasjonssammenligningen sist ble oppdatert."@nb ,
                          "This property is used to specify the date or time when the classification/correspondence was last modified."@en ;
             rdfs:isDefinedBy dct:modified ;
             rdfs:label "modified"@en ,
                        "sist oppdatert"@nb .


###  http://purl.org/dc/terms/publisher
dct:publisher rdf:type owl:DatatypeProperty ;
              rdfs:domain xkos:Correspondence ,
                          skos:ConceptScheme ;
              rdfs:range xsd:anyURI .


###  http://purl.org/dc/terms/replaces
dct:replaces rdf:type owl:DatatypeProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range xsd:anyURI ;
             vann:usageNote "This property is not explicitly specified in XKOS-AP-NO, but included here in order to express the relation replaces/isReplacedBy with a concept outside of the current classification, referenced as an URI."@en ;
             rdfs:isDefinedBy "http://purl.org/dc/terms/replaces" .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:DatatypeProperty ;
          rdfs:domain xkos:Correspondence ,
                      skos:ConceptScheme ;
          rdfs:range rdf:langString .


###  http://purl.org/dc/terms/type
dct:type rdf:type owl:DatatypeProperty ;
         rdfs:domain xkos:ConceptAssociation ;
         rdfs:range xsd:anyURI ;
         vann:usageNote "The value shall be chosen from the common controlled vocabulary https://data.norge.no/vocabulary/classification-item-change-type, when the value is in the vocabulary."@en ,
                        "Verdien skal velges fra det felles kontrollerte vokabularet https://data.norge.no/vocabulary/classification-item-change-type, når verdien finnes i vokabularet."@nb ;
         rdfs:comment "Egenskapen brukes til å oppgi type endring mellom to klassifikasjoner i samme klassifikasjonsfamilie/-serie."@nb ,
                      "This property is used to specify the type of the change between two classifications in the same classification family/series."@en ;
         rdfs:isDefinedBy dct:type ;
         rdfs:label "change type"@en ,
                    "type endring"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#additionalContentNote
xkos:additionalContentNote rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf xkos:inclusionNote ;
                           rdfs:comment "Egenskapen brukes til å oppgi tilleggsinnholdet i kategorien. Egenskapen bør gjentas når teksten er i flere ulike språk."@nb ,
                                        "This property is used to specify additional content to the classification item, repeated when the note is in different languages."@en ;
                           rdfs:label "additional content note"@en ,
                                      "tilleggsinnhold"@nb ;
                           skos:example "Kategori med koden «01» i «Næringsgruppering 2007 (SN 2007)» har følgende note om tilleggsinnhold: «Inkluderer også: Omfatter også tjenester tilknyttet jordbruk, jakt og fangst.»"@nb ,
                                        "xkos:additionalContentNote is generally labelled \"This category includes also\", \"This item includes also\", \"This division includes also\", \"Includes also\", or similar."@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#coreContentNote
xkos:coreContentNote rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf xkos:inclusionNote ;
                     rdfs:comment "Egenskapen brukes til å oppgi hva som er hovedinnholdet i kategorien. Egenskapen bør gjentas når teksten er i flere ulike språk."@nb ,
                                  "This property is used to specify the core content of the classification item, repeated when the note is in different languages."@en ;
                     rdfs:label "core content note"@en ,
                                "hovedinnhold"@nb ;
                     skos:example "Kategori med koden «01» i «Næringsgruppering 2007 (SN 2007)» har følgende note om hovedinnhold: \"Inkluderer: Denne næringen omfatter to basisaktiviteter: produksjon av vegetabilske og animalske produkter, jordbruk, dyrking av genetisk modifiserte vekster og oppdrett av genetisk modifiserte dyr. Både dyrking av vekster på friland og i veksthus inngår.\""@nb ,
                                  "xkos:coreContentNote is generally labelled \"This category includes\", \"This item includes\", \"This division includes\", \"Includes\" or similar."@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#depth
xkos:depth rdf:type owl:DatatypeProperty ;
           rdfs:domain xkos:ClassificationLevel ;
           rdfs:range xsd:positiveInteger ;
           vann:usageNote "Det ene nivået i en flat klassifikasjon har verdi 1 som sitt nivånummer."@nb ,
                          "The only one level in a flat classification has value 1 as the depth."@en ;
           rdfs:comment "Egenskapen brukes til å oppgi nivånummer av det aktuelle klassifikasjonsnivået, dvs. avstanden fra dette nivået til «roten» av klassifikasjonen. 1 for det høyeste nivået."@nb ,
                        "This property is used to specify the depth of a level inside a classification (1 for the highest level)."@en ;
           rdfs:isDefinedBy <http://rdf-vocabulary.ddialliance.org/xkos> ;
           rdfs:label "depth"@en ,
                      "nivånummer"@nb ;
           skos:example "Eksempel"@nb ,
                        "Nivå 4 «Næringsgruppe» i Næringsgruppering har verdi 4 som nivånummer."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#exclusionNote
xkos:exclusionNote rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf skos:scopeNote ;
                   rdfs:comment "Egenskapen brukes til å oppgi hva som ikke er inkludert i kategorien. Egenskapen bør gjentas når merknaden finnes i flere ulike språk."@nb ,
                                "This property is used to specify what is not included in the category, repeated when the note is in different languages."@en ;
                   rdfs:label "eksklusjonsmerknad"@nb ,
                              "exclusion note"@en ;
                   skos:example "Kategori med koden «01» i «Næringsgruppering 2007 (SN 2007)» har følgende eksklusjonsnote: «Ekskluderer: Grunnarbeid, f.eks. anlegg av jordterrasser, drenering o.l. grupperes under næringshovedområde: F Bygge- og anleggsvirksomhet. Kjøpere og andelslag engasjert i markedsføring av jordbruksprodukter grupperes under næringshovedområde: G Varehandel, reparasjon av motorvogner. Stell og vedlikehold av landskap grupperes under: 81.30 Beplantning av hager og parkanlegg.»"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#inclusionNote
xkos:inclusionNote rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf skos:scopeNote ;
                   rdfs:comment "Bruk heller den semantisk sett mer presise egenskapen Kategori – hovedinnhold (xkos:coreContentNote), ev. kombinert med egenskapen Kategori – tilleggsinnhold (xkos:additionalContentNote)."@nb ,
                                "Egenskapen brukes til å oppgi merknad om hva som er inkludert i kodeelementet. Egenskapen bør gjentas når merknaden finnes i flere ulike språk."@nb ,
                                "This property is used to specify what is included in the category, repeated when the note is in different languages."@en ,
                                "Use rather the semantically more precise property xkos:coreContentNote, alternatively in combination with the property xkos:additionalContentNote."@en ;
                   rdfs:label "inclusion note"@en ,
                              "inklusjonsmerknad"@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#maxLength
xkos:maxLength rdf:type owl:DatatypeProperty ;
               rdfs:domain skos:Concept ;
               rdfs:range xsd:positiveInteger ;
               vann:usageNote """According to XKOS:
In all cases the skos:prefLabel property is used to express the full (official) label. For the additional fixed-length labels, skosxl:Label instances are created with the xkos:maxLength property indicating the maximum length of these additional labels. Two cases must be distinguished:
* If the additional label is different from the full label, the skosxl:Label is attached by a skosxl:altLabel property.
* If the additional label is equal to the full label, the skosxl:Label must be attached by a skosxl:prefLabel property in order to comply with the SKOS integrity rules about labels."""@en ,
                              "For deg som utvikler/tilpasser verktøystøtte, se engelsk note."@nb ;
               rdfs:comment "Egenskapen brukes til å oppgi maksimum kodelengde."@nb ,
                            "This property is used to specify the maximum number of characters for a label that is constrained in length."@en ;
               rdfs:isDefinedBy xkos:maxLength ;
               rdfs:label "maksimum kodelengde"@nb ,
                          "maximum length"@en .


###  http://rdf-vocabulary.ddialliance.org/xkos#notationPattern
xkos:notationPattern rdf:type owl:DatatypeProperty ;
                     rdfs:domain xkos:ClassificationLevel ;
                     rdfs:range rdfs:Literal ;
                     rdfs:comment "Classification items of a given levels usually have a code (expressed by the skos:notation property) that conforms to a specific structure. In order to capture this information, XKOS defines the xkos:notationPattern property. This property is attached to a classification level and should contain a regular expression reflecting the code structure of the items of this level."@en ,
                                  "Egenskapen brukes til å oppgi mønsteret for kodene på et gitt klassifikasjonsnivå. Verdien bør inneholde et regulært uttrykk."@nb ;
                     rdfs:isDefinedBy xkos:notationPattern ;
                     rdfs:label "code pattern"@en ,
                                "kodemønster"@nb ;
                     skos:example "For example, the NACE sections are identified by a capital letter between A and U."@en ,
                                  "Kodene på nivå 1 i «Næringsgruppering 2007» er én stor bokstav (fra A til U)."@nb .


###  http://rdf-vocabulary.ddialliance.org/xkos#numberOfLevels
xkos:numberOfLevels rdf:type owl:DatatypeProperty ;
                    rdfs:domain skos:ConceptScheme ;
                    rdfs:range xsd:positiveInteger ;
                    rdfs:comment "Egenskapen brukes til å oppgi antall nivåer i klassifikasjonen; 1 for en flat klassifikasjon."@nb ,
                                 "This property is used to document number of levels the classification includes; 1 for a flat classification."@en ;
                    rdfs:isDefinedBy xkos:numberOfLevels ;
                    rdfs:label "antall nivåer"@nb ,
                               "number of levels"@en ;
                    skos:example "«Næringsgruppering 2007» har totalt fem nivåer."@nb .


###  http://schema.org/validFrom
<http://schema.org/validFrom> rdf:type owl:DatatypeProperty ;
                              rdfs:domain skos:ConceptScheme ;
                              rdfs:range xsd:date ,
                                         xsd:dateTime ;
                              rdfs:comment "Egenskapen brukes til å oppgi fra og med når klassifikasjonen er gyldig."@nb ,
                                           "This property is used to specify the date or time from (inclusive) which the classification is valid."@en ;
                              rdfs:isDefinedBy schema:validFrom ;
                              rdfs:label "gyldig til og med"@nb ,
                                         "valid from (inclusive)"@en .


###  http://schema.org/validThrough
<http://schema.org/validThrough> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain skos:ConceptScheme ;
                                 rdfs:range xsd:date ,
                                            xsd:dateTime ;
                                 rdfs:comment "Egenskapen brukes til å oppgi fra og med når klassifikasjonen er gyldig."@nb ,
                                              "This property is used to specify the date or time from (inclusive) which the classification is valid."@en ;
                                 rdfs:isDefinedBy schema:validThrough ;
                                 rdfs:label "gyldig til og med"@nb ,
                                            "valid through (inclusive)"@en .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:DatatypeProperty ;
              rdfs:domain skos:Concept ;
              rdfs:range rdf:langString ;
              rdfs:comment "Egenskapen brukes til å oppgi alternativt kategorinavn (som kan brukes ved siden av det anbefalte kategorinavnet). Egenskapen bør gjentas når navnet finnes i flere ulike språk."@nb ,
                           "This property is used to specify alternative label of the category, repeated when the label is in different languages."@en ;
              rdfs:isDefinedBy skos:altLabel ;
              rdfs:label "alternative label"@en ,
                         "tillatt kategorinavn/term, synonym"@nb ;
              skos:example "Kategorien med kode «D» og anbefalt tekst «Elektrisitets-, gass-, damp- og varmtvannsforsyning» i «Næringsgruppering 2007» har tillatt kategorinavn «Kraftforsyning»."@nb .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:DatatypeProperty ;
                rdfs:domain skos:Concept ;
                rdfs:range rdf:langString ;
                vann:usageNote "Bruk heller egenskapen Kategori – refererer til begrep (dct:subject) til å referere til en begrepsbeskrivelse som er tilgjengeliggjort f.eks. i begrepsdelen av Felles datakatalog og som inneholder definisjonen av begrepet, istedenfor å duplisere/«gjemme» definisjonen i en klassifikasjon."@nb ,
                               "Use rather the property dct:subject to refer to a concept which is available in e.g. Felles datakatalog and which contains the defintion of the concept, instead of duplicating/\"hiding\" the definition in a classification."@en ;
                rdfs:comment "Egenskapen brukes til å oppgi definisjonen av begrepet som kategorien representerer. Egenskapen bør gjentas når definisjonen finnes i flere ulike språk."@nb ,
                             "This property is used to specify the definition of the concept that the category represents, repeated when the definition is in different languages."@en ;
                rdfs:isDefinedBy skos:definition ;
                rdfs:label "definisjon"@nb ,
                           "definition"@en .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:DatatypeProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range rdf:langString .


###  http://www.w3.org/2004/02/skos/core#hiddenLabel
skos:hiddenLabel rdf:type owl:DatatypeProperty ;
                 rdfs:domain skos:Concept ;
                 rdfs:range rdf:langString ;
                 rdfs:comment "Egenskapen brukes til å oppgi kategorinavn som anses som uegnet for kategorien. EGenskapen bør gjentas når navnet finnes i flere ulike språk."@nb ,
                              "This property is used to specify the label that is considered as not suitable for the classification item, repeated when the label is in different languages."@en ;
                 rdfs:isDefinedBy skos:hiddenLabel ;
                 rdfs:label "frarådet kategorinavn/term"@nb ,
                            "hidden label"@en .


###  http://www.w3.org/2004/02/skos/core#notation
skos:notation rdf:type owl:DatatypeProperty ;
              rdfs:domain skos:Concept ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "Egenskapen brukes til å oppgi koden for kategorien."@nb ,
                           "This property is used to specify the code for the classification item."@en ;
              rdfs:isDefinedBy skos:notation ;
              rdfs:label "code"@en ,
                         "kode"@nb ;
              skos:example "«A» er koden for kategorien med kategorinavn «Jordbruk, skogbruk og fiske», på nivå 1 i «Næringsgruppering 2007 (SN2007)»."@nb .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:DatatypeProperty ;
          rdfs:domain skos:Concept ;
          rdfs:range rdf:langString ;
          vann:usageNote "Egenskapen brukes kun til merknader som ikke kan tas med i en av de andre semantisk sett mer presise egenskapene som Kategori – eksklusjonsmerknad (skos:exclusionNote), Kategori – hovedinnhold (xkos:coreContentNote) og Kategori – tilleggsinnhold (xkos:additionalContentNote)."@nb ,
                         "Use this property only for notes that may not be given as values to one of the other semantically more precise properties such as skos:exclusionNote, xkos:coreContentNote and xkos:additionalContentNote."@en ;
          rdfs:comment "Egenskapen brukes til å oppgi merknad om kategorien. Egenskapen bør gjentas når merknaden finnes i flere ulike språk/målformer."@nb ,
                       "This property is used to specify any note about the classification item, repeated when the note is in different languages."@en ;
          rdfs:label "merknad"@nb ,
                     "note"@en .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:DatatypeProperty ;
               rdfs:domain skos:Concept ;
               rdfs:range rdf:langString .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf skos:note ;
               vann:usageNote "This property is not explicitly specified in XKOS-AP-NO, but included here in order to have it as the super-property for other properties, e.g. xkos:inclusionNote."@en ;
               rdfs:isDefinedBy skos:scopeNote .


#################################################################
#    Classes
#################################################################

###  http://rdf-vocabulary.ddialliance.org/xkos#ClassificationLevel
xkos:ClassificationLevel rdf:type owl:Class ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:covers ;
                                           owl:someValuesFrom skos:Concept
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:coversExhaustively ;
                                           owl:someValuesFrom skos:Concept
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:coversMutuallyExclusively ;
                                           owl:someValuesFrom skos:Concept
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty skos:member ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass skos:Concept
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:organizedBy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass skos:Concept
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:notationPattern ;
                                           owl:someValuesFrom rdfs:Literal
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty xkos:depth ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:positiveInteger
                                         ] ;
                         vann:usageNote "A flat classification is considered as a classifcation with one level."@en ,
                                        "Flate klassifikasjoner betraktes til å inneha ett nivå."@nb ;
